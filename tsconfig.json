{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "rootDirs": ["./script_ts"],
    "outDir": "./script_js",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "removeComments": true,
    "noEmitOnError": true
  },
  "include": ["script_ts/**/*"],
  "exclude": ["script_js", "node_modules"]
}