{
  "name": "calculadora",
  "version": "1.0.0",
  "description": "Projeto-calculadora",
  "keywords": [
    "calculadora",
    "vanilla",
    "typescript",
    "pwa"
  ],
  "homepage": "https://github.com/Anathyon/Calculadora#readme",
  "bugs": {
    "url": "https://github.com/Anathyon/Calculadora/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anathyon/Calculadora.git"
  },
  "license": "ISC",
  "author": "Anathyon Erysson",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "dev": "live-server --port=3000",
    "build": "npm run build:ts && npm run build:css",
    "build:ts": "tsc",
    "build:css": "sass CSS/index.scss CSS/index.css --style=compressed",
    "watch": "concurrently \"tsc --watch\" \"sass --watch CSS/index.scss:CSS/index.css\"",
    "start": "npm run build && npm run dev"
  },
  "dependencies": {
    "live-server": "^1.2.2"
  },
  "devDependencies": {
    "sass": "^1.94.2",
    "typescript": "^5.0.0",
    "concurrently": "^8.0.0"
  },
  "devDependencies": {
    "sass": "^1.94.2"
  }
}
